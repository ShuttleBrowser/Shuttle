{
  "name": "shuttle",
  "version": "2.0.6",
  "description": "The fastest access to your favorite applications",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "build -mwl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robjullian/shuttle.git"
  },
  "keywords": [
    "js",
    "javascript",
    "electron"
  ],
  "author": "Robin Jullian <robin.jullian@getshuttle.xyz>",
  "license": "NPOSL-3.0",
  "bugs": {
    "url": "https://github.com/robjullian/shuttle/issues"
  },
  "homepage": "https://getshuttle.xyz/",
  "dependencies": {
    "auto-launch": "^5.0.5",
    "electron-localshortcut": "^3.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^2.20.1",
    "favicon": "0.0.2",
    "lowdb": "^1.0.0",
    "menubar": "^5.2.3",
    "os-locale": "^2.1.0",
    "request": "^2.83.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-builder": "^20.0.5",
    "electron-packager": "^10.1.2",
    "standard": "^10.0.3"
  },
  "build": {
    "appId": "xyz.robjullian.shuttle",
    "compression": "maximum",
    "asar": "false",
    "win": {
      "target": [
        "NSIS",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "tar.gz"
      ],
      "icon": "./assets/img/icon.png",
      "synopsis": "The fastest access to your favorite applications",
      "description": "The fastest access to your favorite applications",
      "category": "Network"
    },
    "mac": {
      "category": "public.app-category.productivity"
    }
  }
}
