{
  "name": "Shuttle",
  "version": "0.0.1",
  "description": "The fastest access to your favorites applications.",
  "main": "main.js",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "electron": "^1.7.9",
    "electron-debug": "^1.4.0",
    "electron-dl": "^1.10.0",
    "electron-simple-updater": "^1.2.2",
    "electron-winstaller": "^2.6.3",
    "fs": "0.0.1-security",
    "get-image-colors": "^1.8.1",
    "http": "0.0.0",
    "jquery": "^3.2.1",
    "jsonfile": "^4.0.0",
    "menubar": "^5.2.3",
    "path": "^0.12.7",
    "replace": "^0.3.0",
    "url": "^0.11.0",
    "vex-js": "^4.0.1"
  },
  "devDependencies": {},
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Shuttle --overwrite --platform=win32 --arch=x64 --icon=res/icon.ico --out=release-builds --version-string.CompanyName=Shuttle_ltd --version-string.ProductName=\"Shuttle\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=res/logo.png --prune=true --out=release-builds"
  },
